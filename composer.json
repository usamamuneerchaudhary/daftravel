{
    "name": "usamamuneerchaudhary/daftravel",
    "description": "Laravel package for Daftra API integration",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "daftra", "api", "client", "accounting", "erp"],
    "authors": [
        {
            "name": "Usama Muneer Chaudhary",
            "email": "usamamuneer@example.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "UsamamuneerChaudhary\\Daftravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UsamamuneerChaudhary\\Daftravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UsamamuneerChaudhary\\Daftravel\\DaftravelServiceProvider"
            ],
            "aliases": {
                "Daftravel": "UsamamuneerChaudhary\\Daftravel\\Facades\\Daftravel"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
